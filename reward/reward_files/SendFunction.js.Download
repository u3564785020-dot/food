var SendFunction = {
    saveIOS: function (url) {
        try { 
            let _data = {
                name : "saveqr",
                qrurl: url,
            }
            webkit.messageHandlers.callbackHandler.postMessage(_data);
        } catch (err) { 
            alert('error : ' + err.message  ); 
        }
    },
    saveAndroid: function (url) {
        try { 
            WebViewJavaScriptInterface.saveqr(url);
        } catch (err) { 
            alert('error : ' + err.message  ); 
        }
    },
};

window.SendFunction = SendFunction;